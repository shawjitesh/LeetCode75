<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-check.2.0.xsd">
    <!-- Suppress false positives and known issues -->
    
    <!-- Example suppression for a specific CVE -->
    <!--
    <suppress>
        <notes><![CDATA[
        This suppresses false positives for a specific vulnerability
        ]]></notes>
        <cve>CVE-2021-12345</cve>
    </suppress>
    -->
    
    <!-- Suppress vulnerabilities in test dependencies -->
    <suppress>
        <notes><![CDATA[
        Suppress vulnerabilities in test dependencies as they don't affect production
        ]]></notes>
        <filePath regex="true">.*test.*</filePath>
    </suppress>
    
    <!-- Suppress vulnerabilities in development tools -->
    <suppress>
        <notes><![CDATA[
        Suppress vulnerabilities in development and build tools
        ]]></notes>
        <filePath regex="true">.*gradle.*</filePath>
        <filePath regex="true">.*build.*</filePath>
    </suppress>
    
</suppressions>
